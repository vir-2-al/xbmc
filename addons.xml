<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<addons>

	<addon id="repository.virtual" 
	       name="VIRTUAL Add-ons" 
		   version="1.0.0" 
		   provider-name="VIRTUAL">
	<requires>
	  <import addon="xbmc.addon" version="12.0.0"/>
	</requires>	
		<extension point="xbmc.addon.repository" name="VIRTUAL XBMC Add-ons">
			<info compressed="false">https://github.com/vir-2-al/script.module.patch/raw/main/addons.xml</info>
			<checksum>https://github.com/tdw1980/tdw/raw/master/addons.xml.md5</checksum>
			<datadir zip="true">https://github.com/tdw1980/tdw/raw/master/</datadir>
		</extension>
		<extension point="xbmc.addon.metadata">
			<summary>Install Add-ons from VIRTUAL Add-on Repository</summary>
			<description>Plug-ins for XBMC</description>
			<disclaimer>There are git my add-ons for XBMC</disclaimer>
			<platform>all</platform>
		</extension>
	</addon>
	
</addons>